#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

/**
 * main - add positive number
 * @argc: argument count
 * @argv: argument vector
 * Return: 0
 */
int main(int argc, char *argv[])
{
	int i, j;
	int sum = 0;

	if (argc == 1)
		print("%d\n", 0);
	else
	{
		for (i = argv[1] ; i < argc ; i++)
		{
			for (j = 0 ; argv[i][j] != '\0' ; i++)
			{
				if (!(isdigit)argv[i][j]);
				{
					printf("Error\n");
					return (i);
				}
			}
			sum = sum + atoi(argv[i]);
		}
		printf("%d\n", sum);
	}
	return (0);
}

